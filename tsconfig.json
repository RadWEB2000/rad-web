
{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "app/*":["./app/*"],
      "assets/*":["./src/assets/*"],
      "buttons/*":["./src/components/Utils/Buttons/*"],
      "c-blog/*":["./src/components/Utils/Cards/BlogCards/*"],
      "c-case-study/*":["./src/components/Utils/Cards/CaseStudyCard/*"],
      "c-person/*":["./src/components/Utils/Cards/PersonCards/*"],
      "c-project/*":["./src/components/Utils/Cards/ProjectCards/*"],
      "c-service/*":["./src/components/Utils/Cards/ServiceCards/*"],
      "c-stat/*":["./src/components/Utils/Cards/StatisticCards/*"],
      "c-industry/*":["./src/components/Utils/Cards/IndustryCards/*"],
      "c-directonary/*":["./src/components/Utils/Cards/DirectonaryCard/*"],
      "cards/*":["./src/components/Utils/Cards/*"],
      "components/*":["./src/components/*"],
      "configs/*":["./src/lib/configs/*"],
      "context/*":["./src/context/*"],
      "css/*":["./src/styles/*"],
      "data/*":["./src/data/*"],
      "fonts/*":["./src/assets/fonts/*"],
      "footer/*":["./src/components/Layout/Footer/*"],
      "functions/*":["./src/lib/functions/*"],
      "hooks/*":["./src/lib/hooks/*"],
      "img/*":["./src/assets/images/*"],
      "l-footer/*":["./src/components/Layout/Footer/*"],
      "l-nav/*":["./src/components/Layout/Navigation/*"],
      "l-d-nav/*":["./src/components/Layout/Navigation/DesktopNavigation/*"],
      "l-m-nav/*":["./src/components/Layout/Navigation/MobileNavigation/*"],
      "l-seo/*":["./src/components/Layout/Seo/*"],
      "layout/*":["./src/components/Layout/*"],
      "lib/*":["./src/lib/*"],
      "movies/*":["./src/assets/movies/*"],
      "public/*": ["./public/*"],
      "src/*":["./src/*"],
      "ts/*":["./src/types/*"],
      "utils/*":["./src/components/Utils/*"],
      "v-about/*":["./src/components/Views/AboutView/*"],
      "v-blog/*":["./src/components/Views/BlogView/*"],
      "v-contact/*":["./src/components/Views/ContactPage/*"],
      "v-home/*":["./src/components/Views/HomeView/*"],
      "v-post/*":["./src/components/Views/PostView/*"],
      "v-services/*":["./src/components/Views/ServicesView/*"],
      "v-m-service/*":["./src/components/Views/MainServiceView/*"],
      "v-directory/*":["./src/components/Views/DirectonaryView/*"],
      "v-project/*":["./src/components/Views/ProjectView/*"],
      "v-projects/*":["./src/components/Views/ProjectsView/*"],
      "views/*":["./src/components/Views/*"],
      "l-seo/*":["./src/components/Layout/Seo/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
